<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net10.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>True</UseWPF>
    <ApplicationIcon>Windrose.ico</ApplicationIcon>
    <StartupObject>RuzaVjetrova.App</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="languages.json" />
    <None Remove="WindRose.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="languages.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Windrose.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="WindRose.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
